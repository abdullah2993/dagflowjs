  "license": "MIT",
  "homepage": "https://github.com/abdullah2993/dagflowjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdullah2993/dagflowjs.git"
  },
  "bugs": {
    "url": "https://github.com/abdullah2993/dagflowjs/issues"
  },
  "exports": {
